#nameservers
  should: %s CLASS(array)
  should: %s SIZE(4)
  should: %s[0] CLASS(nameserver)
  should: %s[0].name == "ns-1and1.ui-dns.com"
  should: %s[0].ipv4 == "217.160.82.200"
  should: %s[1] CLASS(nameserver)
  should: %s[1].name == "ns-1and1.ui-dns.org"
  should: %s[1].ipv4 == nil
  should: %s[2] CLASS(nameserver)
  should: %s[2].name == "ns-1and1.ui-dns.de"
